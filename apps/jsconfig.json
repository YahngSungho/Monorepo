{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"allowJs": true,
		"checkJs": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"baseUrl": ".",
		"strict": false,
		"noImplicitAny": false,
		"typeRoots": [
			"./node_modules/@types",
			"./types"
		],
		"skipLibCheck": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
	},
	"include": [
		"./**/*"
, "../../packages/library_wrappers/random_engine.js"	],
	"exclude": [
		"node_modules",
		"dist",
		".history"
	]
}