{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"paths": {
			"$shadcn/*": [
				"./src/shadcn/*"
			],
			"$shadcn": [
				"./src/shadcn"
			],
		}
	}
}