{
	"$schema": "https://turbo.build/schema.json",
	"extends": ["//"],
	"tasks": {
		"build": {
			"inputs": [
				"$TURBO_DEFAULT$",
				"!**/*.stories.{js,ts,svelte,tsx,jsx,mdx}",
				"**/.env.*",
