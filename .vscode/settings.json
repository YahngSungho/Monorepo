{
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[gitignore]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[ignore]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[svelte]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"bookmarks.useWorkaroundForFormatters": true,
	"console-ninja.dateTimeDisplayMode": "Relative To Previous",
	"cssVariables.lookupFiles": [
		"libraries/base/node_modules/open-props/open-props.min.css",
		"libraries/base/base.css",
		"**/*.css",
		"**/*.scss",
		"**/*.sass",
		"**/*.less"
	],
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"eslint.format.enable": true,
	"eslint.lintTask.enable": true,
	"eslint.probe": [
		"astro",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"html",
		"mdx",
		"vue",
		"markdown",
		"json",
		"jsonc",
		"svelte"
	],
	"eslint.run": "onSave",
	"eslint.useESLintClass": true,
	"eslint.useFlatConfig": true,
	"eslint.validate": [
		"css",
		"svelte",
		"html",
		"postcss",
		"jsx",
		"tsx",
		"js",
		"ts",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"javascriptsvelte",
		"json",
		"jsonc",
		"json5",
		"yaml",
		"yml",
		"toml"
	],
	"peacock.color": "#920320",
	"prettier.configPath": "",
	"prettier.experimentalTernaries": false,
	"prettier.jsxSingleQuote": true,
	"prettier.semi": false,
	"prettier.singleAttributePerLine": false,
	"prettier.singleQuote": true,
	"prettier.useTabs": true,
	"prettier.vueIndentScriptAndStyle": false,
	"stylelint.validate": ["css", "scss", "svelte"],
	"svelte.plugin.svelte.format.config.singleQuote": true,
	"svelte.plugin.svelte.format.config.svelteAllowShorthand": true,
	"svelte.plugin.svelte.format.config.svelteIndentScriptAndStyle": true,
	"typescript.tsdk": "node_modules/typescript/lib",
	"workbench.colorCustomizations": {
		"activityBar.activeBackground": "#c4042b",
		"activityBar.background": "#c4042b",
		"activityBar.foreground": "#e7e7e7",
		"activityBar.inactiveForeground": "#e7e7e799",
		"activityBarBadge.background": "#29bb04",
		"activityBarBadge.foreground": "#e7e7e7",
		"commandCenter.border": "#e7e7e799",
		"editorGroup.border": "#c4042b",
		"sash.hoverBorder": "#c4042b",
		"sideBar.border": "#c4042b",
		"statusBar.background": "#920320",
		"statusBar.foreground": "#e7e7e7",
		"statusBarItem.hoverBackground": "#c4042b",
		"statusBarItem.remoteBackground": "#920320",
		"statusBarItem.remoteForeground": "#e7e7e7",
		"titleBar.activeBackground": "#920320",
		"titleBar.activeForeground": "#e7e7e7",
		"titleBar.inactiveBackground": "#92032099",
		"titleBar.inactiveForeground": "#e7e7e799"
	}
}
