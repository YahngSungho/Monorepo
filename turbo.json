{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [],
    "globalEnv": [
        "NODE_ENV"
    ],
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "inputs": [
                "$TURBO_DEFAULT$",
								"!**/*.stories.{js,ts,svelte,tsx,jsx,mdx}",
								".env.production.local",
								".env.local",
								".env.production",
								".env"
            ],
            "outputs": [
								"dist",
                ".svelte-kit/**",
                ".vercel/**"
            ]
        },
        "build:storybook": {
            "dependsOn": [
                "^build:storybook"
            ],
            "outputs": [
                "storybook-static/**"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "lint": {},
        "test": {}
    }
}